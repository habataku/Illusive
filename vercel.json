{
  "regions": ["lhr1"],
  "builds": [
    { "src": "/server.js","use": "@vercel/node" },
    { "src": "public/**","use": "@vercel/static" },
    { "src": "*.json","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/public/frame.html" },
    { "src": "/uv/(.*)","dest": "/public/uv/$1" },
    { "src": "/.*","dest": "/server.js" }
  ]
}
